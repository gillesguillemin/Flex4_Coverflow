<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009"
				xmlns:mx="library://ns.adobe.com/flex/halo"
				xmlns:s="library://ns.adobe.com/flex/spark">

	<s:states>
		<s:State name="normal" />
	</s:states>

	<s:Panel title="{data}"
			 x="-100"
			 width="200"
			 height="200">
		<s:Label text="{data}"
				 horizontalCenter="0"
				 verticalCenter="0"
				 fontSize="130" />
	</s:Panel>

</s:ItemRenderer>
